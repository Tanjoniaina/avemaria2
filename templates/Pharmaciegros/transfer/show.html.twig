{% extends 'base.html.twig' %}

{% block title %}Transfer{% endblock %}

{% block body %}
    <h1>Transfer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transfer.id }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ transfer.status }}</td>
            </tr>
            <tr>
                <th>TransfertDate</th>
                <td>{{ transfer.transfertDate ? transfer.transfertDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ transfer.comment }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pharmaciegros_entity_transfer_index') }}">back to list</a>

    <a href="{{ path('app_pharmaciegros_entity_transfer_edit', {'id': transfer.id}) }}">edit</a>

    {{ include('app/pharmaciegros/transfer/_delete_form.html.twig') }}
{% endblock %}
