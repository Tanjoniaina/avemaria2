{% extends 'base.html.twig' %}

{% block title %}Tarifacte index{% endblock %}

{% block body %}
    <h1>Tarifacte index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Montant</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tarifacte in tarifactes %}
            <tr>
                <td>{{ tarifacte.id }}</td>
                <td>{{ tarifacte.nom }}</td>
                <td>{{ tarifacte.montant }}</td>
                <td>
                    <a href="{{ path('app_shared_entity_tarifacte_show', {'id': tarifacte.id}) }}">show</a>
                    <a href="{{ path('app_shared_entity_tarifacte_edit', {'id': tarifacte.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_shared_entity_tarifacte_new') }}">Create new</a>
{% endblock %}
