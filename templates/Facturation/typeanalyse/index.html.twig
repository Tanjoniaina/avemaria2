{% extends 'base.html.twig' %}

{% block title %}Typeanalyse index{% endblock %}

{% block body %}
    <h1>Typeanalyse index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Montant</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for typeanalyse in typeanalyses %}
            <tr>
                <td>{{ typeanalyse.id }}</td>
                <td>{{ typeanalyse.nom }}</td>
                <td>{{ typeanalyse.montant }}</td>
                <td>
                    <a href="{{ path('app_facturation_entity_typeanalyse_show', {'id': typeanalyse.id}) }}">show</a>
                    <a href="{{ path('app_facturation_entity_typeanalyse_edit', {'id': typeanalyse.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_facturation_entity_typeanalyse_new') }}">Create new</a>
{% endblock %}
